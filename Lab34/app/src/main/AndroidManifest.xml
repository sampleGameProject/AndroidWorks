<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kalashnikov_po2.lab34" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".ShaderPreferencesActivity"
            android:label="@string/title_activity_shader_preferences"
            android:parentActivityName=".StartActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="kalashnikov_po2.lab34.StartActivity" />
        </activity>
        <activity
            android:name=".JSONActivity"
            android:label="@string/title_activity_json"
            android:parentActivityName=".StartActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="kalashnikov_po2.lab34.StartActivity" />
        </activity>
        <activity
            android:name=".CSVActivity"
            android:label="@string/title_activity_csv"
            android:parentActivityName=".StartActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="kalashnikov_po2.lab34.StartActivity" />
        </activity>
    </application>

</manifest>
